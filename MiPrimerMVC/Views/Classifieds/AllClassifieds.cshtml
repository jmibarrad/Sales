                    </ul>
                    <div class="clearfix"></div>
                    <!-- // Top Menu Right END -->

                </div>

            </div>
            <!-- Top navbar END --><div class="container-960 innerT">

                <h3 class="glyphicons shopping_cart"><i></i>Online Shop</h3>
                <div class="separator bottom"></div>

                <div class="widget widget-heading-simple widget-body-gray">
                    <div class="widget-body"><p class="lead center margin-none">All Classifieds shown below. <i style="color:orange" class="fa fa-arrow-circle-o-down"></i></p></div>
                </div>	
                <div class="separator bottom"></div>


                <div class="row-fluid">
                    <div class="span3">

                        <!-- Widget -->
                        <div class="widget widget-heading-simple widget-body-gray">
                            <div class="widget-body list">

                                <!-- List -->
                                <ul>
                                    <li>
                                        <a href=""><i class="fa fa-car"></i> @Html.ActionLink(" Automoviles", "AllClassifieds", "Classifieds", new { @category = "Auto" }, null)</a>
                                        <span class="badge">35</span>
                                    </li>
                                    <li>
                                        <a href=""><i class="fa fa-music"></i> @Html.ActionLink(" Instruments", "AllClassifieds", "Classifieds", new { @category = "Inst" }, null)</a>
                                        <span class="badge">30</span>
                                    </li>
                                    <li>
                                        <a href=""><i class="fa fa-gamepad"></i>@Html.ActionLink(" VG. COnsoles", "AllClassifieds", "Classifieds", new { @category = "VGC" }, null)</a>
